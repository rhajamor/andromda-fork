<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>AndroMDA Bootstrap Plugin</name>
  <description>A plugin used to run AndroMDA in &quot;bootstrap&quot; mode</description>
  <groupId>org.andromda.maven.plugins</groupId>
  <artifactId>andromda-bootstrap-plugin</artifactId>
  <version>3.5-SNAPSHOT</version>
  <goalPrefix>andromda-bootstrap</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>test-translation-library</goal>
      <description>The bootstrap version of the translation-library test.</description>
      <requiresDependencyResolution>test</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>test</phase>
      <implementation>org.andromda.maven.plugin.bootstrap.TranslationLibraryTestMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>configurationUri</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>This is the URI to the AndroMDA configuration file.</description>
        </parameter>
        <parameter>
          <name>localRepository</name>
          <type>org.apache.maven.artifact.repository.ArtifactRepository</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>plugins</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>The registered plugin implementations.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>propertyFiles</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>reportDirectory</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Base directory to which the cartridge test report is written</description>
        </parameter>
        <parameter>
          <name>settings</name>
          <type>org.apache.maven.settings.Settings</type>
          <required>true</required>
          <editable>false</editable>
          <description>The current user system settings for use in Maven. (allows us to pass the user
settings to the AndroMDA configuration).</description>
        </parameter>
        <parameter>
          <name>skip</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to &apos;true&apos; to bypass cartridge tests entirely. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
        <parameter>
          <name>skipTests</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to &apos;true&apos; to skip running tests, but still compile them. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
        <parameter>
          <name>testFailureIgnore</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to true to ignore a failure during testing. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
        <parameter>
          <name>testSourceDirectory</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>false</editable>
          <description>The directory containing the test source.</description>
        </parameter>
        <parameter>
          <name>traceExpression</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Whether or not the expression shall be &apos;traced&apos; (i.e. the TraceTranslator will run instead of the specified translator).
This is helpful, in allowing us to see which expressions are being parsed in what order, etc.</description>
        </parameter>
        <parameter>
          <name>translationName</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>When specified, only this translation will be tested (If more than one TestTranslation-* file is found).</description>
        </parameter>
      </parameters>
      <configuration>
        <configurationUri implementation="java.lang.String">file:${basedir}/conf/test/andromda.xml</configurationUri>
        <localRepository implementation="org.apache.maven.artifact.repository.ArtifactRepository">${localRepository}</localRepository>
        <plugins implementation="java.util.List">${project.build.plugins}</plugins>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <propertyFiles implementation="java.util.List">${project.build.filters}</propertyFiles>
        <reportDirectory implementation="java.lang.String">${project.build.directory}/translation-library-test/reports</reportDirectory>
        <settings implementation="org.apache.maven.settings.Settings">${settings}</settings>
        <skip implementation="boolean">${maven.test.skip}</skip>
        <skipTests implementation="boolean">${skipTests}</skipTests>
        <testFailureIgnore implementation="boolean" default-value="false">${maven.test.failure.ignore}</testFailureIgnore>
        <testSourceDirectory implementation="java.lang.String">${basedir}</testSourceDirectory>
        <traceExpression implementation="boolean">${trace.expression}</traceExpression>
        <translationName implementation="java.lang.String">${translation.name}</translationName>
      </configuration>
      <requirements>
        <requirement>
          <role>org.apache.maven.artifact.factory.ArtifactFactory</role>
          <field-name>factory</field-name>
        </requirement>
      </requirements>
    </mojo>
    <mojo>
      <goal>run</goal>
      <description>This is exactly the same as the regular AndroMDAMojo in the
andromda-maven-plugin, however this is the &lt;em&gt;bootstrap&lt;/em&gt; plugin which
is used to run AndroMDA in bootstrap mode (with the bootstrap artifacts).</description>
      <requiresDependencyResolution>runtime</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <phase>generate-sources</phase>
      <implementation>org.andromda.maven.plugin.bootstrap.AndroMDAMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>buildSourceDirectory</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The directory to which the build source is located (any generated
source).</description>
        </parameter>
        <parameter>
          <name>configurationUri</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>This is the URI to the AndroMDA configuration file.</description>
        </parameter>
        <parameter>
          <name>lastModifiedCheck</name>
          <type>boolean</type>
          <required>true</required>
          <editable>true</editable>
          <description>Whether or not a last modified check should be performed before running
AndroMDA again.</description>
        </parameter>
        <parameter>
          <name>localRepository</name>
          <type>org.apache.maven.artifact.repository.ArtifactRepository</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>modelOutputHistory</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>The directory where the model generation output history is located
(Modelname file containing a list of files generated by that model).</description>
        </parameter>
        <parameter>
          <name>plugins</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>The registered plugin implementations.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>propertyFiles</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>settings</name>
          <type>org.apache.maven.settings.Settings</type>
          <required>true</required>
          <editable>false</editable>
          <description>The current user system settings for use in Maven. (allows us to pass the
user settings to the AndroMDA configuration).</description>
        </parameter>
      </parameters>
      <configuration>
        <buildSourceDirectory implementation="java.io.File">${project.build.directory}/src/main/java</buildSourceDirectory>
        <configurationUri implementation="java.lang.String">file:${basedir}/conf/andromda.xml</configurationUri>
        <lastModifiedCheck implementation="boolean">false</lastModifiedCheck>
        <localRepository implementation="org.apache.maven.artifact.repository.ArtifactRepository">${localRepository}</localRepository>
        <modelOutputHistory implementation="java.io.File">${project.build.directory}/history</modelOutputHistory>
        <plugins implementation="java.util.List">${project.build.plugins}</plugins>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <propertyFiles implementation="java.util.List">${project.build.filters}</propertyFiles>
        <settings implementation="org.apache.maven.settings.Settings">${settings}</settings>
      </configuration>
      <requirements>
        <requirement>
          <role>org.apache.maven.artifact.factory.ArtifactFactory</role>
          <field-name>factory</field-name>
        </requirement>
      </requirements>
    </mojo>
    <mojo>
      <goal>help</goal>
      <description>Display help information on andromda-bootstrap-plugin.&lt;br/&gt; Call &lt;pre&gt;  mvn andromda-bootstrap:help -Ddetail=true -Dgoal=&amp;lt;goal-name&amp;gt;&lt;/pre&gt; to display parameter details.</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>false</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.andromda.maven.plugin.bootstrap.HelpMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>true</threadSafe>
      <parameters>
        <parameter>
          <name>detail</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>If &lt;code&gt;true&lt;/code&gt;, display all settable properties for each goal.</description>
        </parameter>
        <parameter>
          <name>goal</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>The name of the goal for which to show help. If unspecified, all goals will be displayed.</description>
        </parameter>
        <parameter>
          <name>indentSize</name>
          <type>int</type>
          <required>false</required>
          <editable>true</editable>
          <description>The number of spaces per indentation level, should be positive.</description>
        </parameter>
        <parameter>
          <name>lineLength</name>
          <type>int</type>
          <required>false</required>
          <editable>true</editable>
          <description>The maximum length of a display line, should be positive.</description>
        </parameter>
      </parameters>
      <configuration>
        <detail implementation="boolean" default-value="false">${detail}</detail>
        <goal implementation="java.lang.String">${goal}</goal>
        <indentSize implementation="int" default-value="2">${indentSize}</indentSize>
        <lineLength implementation="int" default-value="80">${lineLength}</lineLength>
      </configuration>
    </mojo>
  </mojos>
