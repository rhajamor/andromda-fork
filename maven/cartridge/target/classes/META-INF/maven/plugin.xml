<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <name>AndroMDA Cartridge Maven Plugin</name>
  <description>A Maven plugin for performing AndroMDA cartridge related tasks (such
        as running cartridge tests).</description>
  <groupId>org.andromda.maven.plugins</groupId>
  <artifactId>andromda-cartridge-plugin</artifactId>
  <version>3.5-SNAPSHOT</version>
  <goalPrefix>andromda-cartridge</goalPrefix>
  <isolatedRealm>false</isolatedRealm>
  <inheritedByDefault>true</inheritedByDefault>
  <mojos>
    <mojo>
      <goal>update</goal>
      <description>Updates the cartridge expected output with the current cartridge output. Invoke it with
&lt;code&gt;mvn andromda-cartridge:update&lt;/code&gt; when you are inside the cartridge root directory.</description>
      <requiresDependencyResolution>test</requiresDependencyResolution>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>org.andromda.maven.plugin.cartridge.CartridgeTestUpdaterMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <threadSafe>false</threadSafe>
      <parameters>
        <parameter>
          <name>actualDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Specifies the directory that contains the &quot;actual&quot; output (meaning the output
that was currently generated)</description>
        </parameter>
        <parameter>
          <name>binaryOutputSuffixes</name>
          <type>java.lang.String</type>
          <required>false</required>
          <editable>true</editable>
          <description>Defines the extensions of binary files, binary files are checked for presence
and equality, however they aren&apos;t compared as strings, like every other file.</description>
        </parameter>
        <parameter>
          <name>configurationUri</name>
          <type>java.lang.String</type>
          <required>true</required>
          <editable>true</editable>
          <description>This is the URI to the AndroMDA configuration file.</description>
        </parameter>
        <parameter>
          <name>expectedDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>Specifies the directory that contains the &quot;expected&quot; output.</description>
        </parameter>
        <parameter>
          <name>expectedOutputArchive</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>true</editable>
          <description>The location of the archive storing the expected output.</description>
        </parameter>
        <parameter>
          <name>lastModifiedCheck</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to &apos;true&apos; to skip code generation if code has not changed since the latest model date.</description>
        </parameter>
        <parameter>
          <name>project</name>
          <type>org.apache.maven.project.MavenProject</type>
          <required>true</required>
          <editable>false</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>propertyFiles</name>
          <type>java.util.List</type>
          <required>false</required>
          <editable>true</editable>
          <description></description>
        </parameter>
        <parameter>
          <name>reportDirectory</name>
          <type>java.io.File</type>
          <required>false</required>
          <editable>true</editable>
          <description>Base directory to which the cartridge test report is written</description>
        </parameter>
        <parameter>
          <name>settings</name>
          <type>org.apache.maven.settings.Settings</type>
          <required>true</required>
          <editable>false</editable>
          <description>The current user system settings for use in Maven. (allows us to pass the user
settings to the AndroMDA configuration).</description>
        </parameter>
        <parameter>
          <name>skip</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to &apos;true&apos; to bypass cartridge tests entirely. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
        <parameter>
          <name>skipTests</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to &apos;true&apos; to skip running tests, but still compile them. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
        <parameter>
          <name>testFailureIgnore</name>
          <type>boolean</type>
          <required>false</required>
          <editable>true</editable>
          <description>Set this to true to ignore a failure during testing. Its use is NOT RECOMMENDED, but quite convenient on occasion.</description>
        </parameter>
      </parameters>
      <configuration>
        <actualDirectory implementation="java.io.File">${project.build.directory}/cartridge-test/actual</actualDirectory>
        <binaryOutputSuffixes implementation="java.lang.String">jpg,jpeg,gif,png,jar,zip</binaryOutputSuffixes>
        <configurationUri implementation="java.lang.String">file:${basedir}/conf/test/andromdaUML2.xml</configurationUri>
        <expectedDirectory implementation="java.io.File">${project.build.directory}/cartridge-test/expected</expectedDirectory>
        <expectedOutputArchive implementation="java.io.File">${basedir}/src/test/expected/cartridge-outputUML2.zip</expectedOutputArchive>
        <lastModifiedCheck implementation="boolean">${lastModifiedCheck}</lastModifiedCheck>
        <project implementation="org.apache.maven.project.MavenProject">${project}</project>
        <propertyFiles implementation="java.util.List">${project.build.filters}</propertyFiles>
        <reportDirectory implementation="java.io.File">${project.build.directory}/cartridge-test/reports</reportDirectory>
        <settings implementation="org.apache.maven.settings.Settings">${settings}</settings>
        <skip implementation="boolean">${maven.test.skip}</skip>
        <skipTests implementation="boolean">${skipTests}</skipTests>
        <testFailureIgnore implementation="boolean" default-value="false">${maven.test.failure.ignore}</testFailureIgnore>
      </configuration>
      <requirements>
        <requirement>
          <role>org.codehaus.plexus.archiver.manager.ArchiverManager</role>
          <field-name>archiverManager</field-name>
        </requirement>
      </requirements>
    </mojo>
